.content {
  background-color: #FBFFFE;
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  padding: 20rpx;
}

.input-bar {
  padding: 10rpx;
  width: 100%;
  box-sizing: border-box;
  background-color: #F0F4F3;
  color: #252626;
  height: 180rpx;
  font-size: 28rpx;
  display: flex;
  align-items: flex-start
}

.image-bar {
  margin-top: 20rpx;
  display: flex;
  align-items: flex-start;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.image-bar .image {
  width: 233rpx;
  height: 233rpx;
  position: relative;
  margin-right: 5rpx;
}

.image-bar .image:nth-child(3n) {
  margin-right: 0rpx;
}

.image-bar .image:nth-child(n + 3) {
  margin-bottom: 5rpx;
}

.image-bar .image .image-dele-bar {
  position: absolute;
  top: 0;
  right: 0;
  width: 40rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(185, 185, 185, 0.51)
}

.image-bar .image .image-dele-bar text {
  color: #fff;
  font-size: 14px;
}

.release {
  margin-top: 50rpx
}

image {
  width: 100%;
  height: 100%
}